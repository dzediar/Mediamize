    <Style TargetType="{x:Type ToolTip}">
        <Setter Property="HasDropShadow" Value="True"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToolTip">
                    <zw:zBorder x:Name="Border" BorderThickness="1" CornerRadius="0" Margin="0" Padding="0"
                                Background="{DynamicResource {x:Static zw:zCompColors.DefaultGradientBackgroundBrushKey}}"
                                BorderBrush="{DynamicResource {x:Static zw:zCompColors.ControlBorderBrushKey}}">
                        <zw:zBorder Style="{StaticResource ClippedBorderStyle}" CornerRadius="4" Margin="0" Padding="0">
                            <zw:zLabel x:Name="Label" Content="{TemplateBinding Content}" Margin="0" Padding="4"/>
                        </zw:zBorder>
                    </zw:zBorder>
                    <ControlTemplate.Triggers>
                        <Trigger Property="HasDropShadow" Value="true">
                            <Setter TargetName="Border" Property="CornerRadius" Value="4"/>
                            <Setter TargetName="Border" Property="SnapsToDevicePixels" Value="true"/>
                        </Trigger>
                        <DataTrigger Binding="{Binding Content, Mode=OneWay, RelativeSource={RelativeSource Self}, Converter={x:Static zwConverters:zTypeNameConverter.Instance}}" Value="String">
                            <Setter TargetName="Label" Property="ContentTemplate">
                                <Setter.Value>
                                    <DataTemplate>
                                        <zw:zTextBox Text="{Binding Mode=OneWay}" MaxWidth="700" Margin="0" Padding="0"
                                                     TextWrapping="Wrap" FontSize="14" TextAlignment="Justify" 
                                                     Foreground="{DynamicResource {x:Static zw:zCompColors.FocusedControlForegroundBrushKey}}"/>
                                    </DataTemplate>
                                </Setter.Value>
                            </Setter>
                        </DataTrigger>                        
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

