                                                <zw:zToggleButton x:Name="PART_Header" Grid.Column="1" Margin="0" Padding="0" ReadOnly="ReadAndWrite"
                                                                      HorizontalContentAlignment="Left" IsChecked="{Binding IsChecked, ElementName=PART_Expander}"
                                                                      Style="{StaticResource {x:Static zw:zCompRepository.EmptyToggleButtonStyleKey}}">
                                                    <zw:zTextBlock zwView:Translator.Text="{Binding Name, Mode=OneTime}" HorizontalAlignment="Left" Margin="2 4 0 0"/>
                                                </zw:zToggleButton>
                                            </Grid>
                                        </zw:zBorder>

                                        <zw:zBorder x:Name="PART_ItemsBorder" Grid.Row="1" Margin="0,0,0,0" Padding="0" BorderThickness="0">
                                            <i:Interaction.Behaviors>
                                                <zwBehaviors:zAnimationBehavior AnimationDirection="Appearance" AnimationType="VerticalFolding, Fade" AnimationDuration="0:0:0.205" SkipAtLoad="True"
                                                                                    TriggerMode="Triggered" TriggerValue="True" Trigger="{Binding IsChecked, Mode=OneWay, ElementName=PART_Expander}"/>
                                                <zwBehaviors:zAnimationBehavior AnimationDirection="Disappearance" AnimationType="VerticalFolding, Fade" AnimationDuration="0:0:0.205" SkipAtLoad="True"
                                                                                    TriggerMode="Triggered" TriggerValue="False" Trigger="{Binding IsChecked, Mode=OneWay, ElementName=PART_Expander}"/>
                                            </i:Interaction.Behaviors>
                                            <StackPanel Orientation="Vertical" Margin="0">
                                                <zw:zGroupItemsPresenter x:Name="PART_Items" Margin="0"/>
                                            </StackPanel>
                                        </zw:zBorder>

                                    </zw:zGrid>
                                </zw:zBorder>

                                <zw:zBorder x:Name="PART_ItemsBorder2" HorizontalAlignment="Stretch" VerticalAlignment="Top" BorderThickness="0" Margin="0" Padding="0" 
                                            Background="{StaticResource {x:Static zw:zCompColors.DefaultGradientBackgroundBrushKey}}" Visibility="Collapsed">
                                    <StackPanel Orientation="Vertical" Margin="0">
                                        <zw:zGroupItemsPresenter x:Name="PART_ItemsBis" Margin="0"/>
                                    </StackPanel>
                                </zw:zBorder>

                            </zw:zGrid>

                            <ControlTemplate.Triggers>
                                <DataTrigger Binding="{Binding Name, Mode=OneWay}" Value="">
                                    <Setter TargetName="PART_Border" Property="Visibility" Value="Collapsed"/>
                                    <Setter TargetName="PART_ItemsBorder2" Property="Visibility" Value="Visible"/>
                                    <Setter TargetName="PART_ItemsBorder" Property="BorderThickness" Value="0"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Name, Mode=OneWay}" Value="{x:Null}">
                                    <Setter TargetName="PART_Border" Property="Visibility" Value="Collapsed"/>
                                    <Setter TargetName="PART_ItemsBorder2" Property="Visibility" Value="Visible"/>
                                    <Setter TargetName="PART_ItemsBorder" Property="BorderThickness" Value="0"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=ItemCount, Mode=OneWay}" Value="0">
                                    <Setter TargetName="PART_Border" Property="Visibility" Value="Collapsed"/>
                                    <Setter TargetName="PART_ItemsBorder2" Property="Visibility" Value="Collapsed"/>
                                    <Setter TargetName="PART_ItemsBorder" Property="BorderThickness" Value="0"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=IsChecked, Mode=OneWay, ElementName=PART_Expander}" Value="False">
                                    <Setter TargetName="PART_HeaderBorder" Property="Margin" Value="0"/>
                                </DataTrigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </GroupStyle.ContainerStyle>
    </GroupStyle>

</ResourceDictionary>